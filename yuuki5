programa
{
	
	funcao inicio()
	{
          inteiro valor1, valor2
		escreva("Digite o valor 1")
		leia(valor1)
		escreva("Digite o valor 2")
		leia(valor2)
		
		se(valor1>valor2){
			escreva("O maior número é:", valor2,"e o maior número é", valor1)
			}
			senao {
				escreva("O maior número é:", valor1,"e o maior número é", valor2)
				}
		}
}
