programa
{
	
	funcao inicio()
	{
	inteiro numero1,numero2,numero3
	
		escreva("Digite o 1 número: ")
		leia(numero1)
		escreva("Digite o 2 número: ")
		leia(numero2)
		escreva("Digite o 3 número: ")
		leia(numero3)

		se(numero1>numero2 e numero1>numero3){
		escreva("O maior número diogitado foi:", numero1)
		}senao se(numero2>numero1 e numero2>numero3){
		escreva("O maior número diogitado foi:", numero2)
	}senao{
		escreva("O maior número diogitado foi:", numero3)
		}
	}
}
